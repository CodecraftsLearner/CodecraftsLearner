*/draw circle shape using just Console in c# /*

using System;

namespace Shravani
{
    class Program
    {
        public static void Main(string[] args)
        {
            DrawCircle();
            Console.ReadLine();
        }

        static void DrawCircle()
        {
            Console.WriteLine("   ***   ");
            Console.WriteLine(" *     * ");
            Console.WriteLine("*       *");
            Console.WriteLine(" *     * ");
            Console.WriteLine("   ***   ");
        }
    }
}
